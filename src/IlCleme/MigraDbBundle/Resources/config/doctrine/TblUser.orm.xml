<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlCleme\MigraDbBundle\Entity\TblUser" table="tbl_user">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nome" type="string" column="nome" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cognome" type="string" column="cognome" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="login" type="string" column="login" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="checkboxTblGruppi" type="string" column="checkbox_tbl_gruppi" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="checkbox1TblGruppi" type="string" column="checkbox1_tbl_gruppi" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
