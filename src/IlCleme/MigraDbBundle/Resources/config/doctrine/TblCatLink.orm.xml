    <field name="idTblLingua" type="integer" column="id_tbl_lingua" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nome" type="string" column="nome" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="smartyTemplate" type="string" column="smarty_template" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="posizione" type="integer" column="posizione" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
