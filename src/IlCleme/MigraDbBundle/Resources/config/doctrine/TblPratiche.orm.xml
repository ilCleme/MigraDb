<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlCleme\MigraDbBundle\Entity\TblPratiche" table="tbl_pratiche">
    <id name="id" type="boolean" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nome" type="string" column="nome" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cognome" type="string" column="cognome" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pratica" type="integer" column="pratica" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="datains" type="date" column="datains" nullable="true"/>
    <field name="idTblImgStatoPratiche" type="integer" column="id_tbl_img_stato_pratiche" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idTblTipoMutuo" type="integer" column="id_tbl_tipo_mutuo" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
