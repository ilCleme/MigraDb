      <generator strategy="IDENTITY"/>
    </id>
    <field name="idTblLingua" type="integer" column="id_tbl_lingua" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idTblDoc" type="integer" column="id_tbl_doc" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idTblDocProperties" type="integer" column="id_tbl_doc_properties" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="propertyValuesTblDoc" type="string" column="property_values_tbl_doc" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="propertyValueReference" type="integer" column="property_value_reference" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
