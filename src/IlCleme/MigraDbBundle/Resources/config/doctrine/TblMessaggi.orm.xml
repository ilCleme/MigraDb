<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlCleme\MigraDbBundle\Entity\TblMessaggi" table="tbl_messaggi">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idTblMessaggi" type="integer" column="id_tbl_messaggi" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idTblLingua" type="integer" column="id_tbl_lingua" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="mittente" type="string" column="mittente" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="destinatari" type="string" column="destinatari" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="oggetto" type="string" column="oggetto" length="80" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="messaggio" type="text" column="messaggio" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="stato" type="integer" column="Stato" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idTblPage" type="integer" column="id_tbl_page" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="datamod" type="date" column="datamod" nullable="false"/>
  </entity>
</doctrine-mapping>
