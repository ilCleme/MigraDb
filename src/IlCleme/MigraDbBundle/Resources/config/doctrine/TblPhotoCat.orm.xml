<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IlCleme\MigraDbBundle\Entity\TblPhotoCat" table="tbl_photo_cat">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idPadre" type="integer" column="id_padre" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idTblPhotoCat" type="integer" column="id_tbl_photo_cat" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idTblLingua" type="integer" column="id_tbl_lingua" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nome" type="string" column="nome" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="num" type="integer" column="num" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="smartyTemplate" type="string" column="smarty_template" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="posizione" type="integer" column="posizione" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="setLoop" type="boolean" column="set_loop" nullable="true"/>
    <field name="setRandom" type="boolean" column="set_random" nullable="true"/>
  </entity>
</doctrine-mapping>
